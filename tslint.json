{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [
      true, "single", "avoid-escape"
    ],
    "trailing-comma": ["error", "never"],
    "curly": [true, "ignore-same-line"],
    "new-parens": false,
    "object-literal-sort-keys": false,
    "no-unused-expression": false,
    "ordered-imports": false
  }
}
